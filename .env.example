DATABASE_URL="postgresql://johndoe:randompassword@localhost:5432/mydb?schema=public"

# JWT secret ( create with command: openssl rand -base64 64 )
JWT_SECRET=

# Server port
PORT=

# Node environment (development, production, etc.)
NODE_ENV="development"